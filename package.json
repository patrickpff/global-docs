    "multicountry",
    "forms",
    "input",
    "masking",
    "validators",
    "official-documents"
  ],
  "homepage": "https://github.com/patrickpff/global-docs#readme",
  "bugs": {
    "url": "https://github.com/patrickpff/global-docs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patrickpff/global-docs.git"
  },
  "license": "MIT",
  "author": "Patrick Ferreira",
  "sideEffects": false,
  "type": "module",
  "exports": {
